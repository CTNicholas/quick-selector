{
  "name": "quick-selector",
  "version": "1.0.9",
  "description": "Syntactic sugar for querySelector, querySelectorAll, & addEventListener allowing you to select, modify, and add event listeners for multiple elements in one line of code. Tiny package (<1KB GZIP).",
  "homepage": "https://github.com/CTNicholas/quick-selector#readme",
  "bugs": {
    "url": "https://github.com/CTNicholas/quick-selector/issues"
  },
  "source": "src/index.js",
  "main": "dist/qs.js",
  "module": "dist/qs.js",
  "unpkg": "dist/qs.js",
  "scripts": {
    "build": "rollup --config",
    "dev": "rollup --config --watch",
    "test": "jest --watch"
  },
  "author": "CTNicholas",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.11.0",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "jest": "^26.1.0",
    "rollup-plugin-terser": "^7.0.0"
  },
  "dependencies": {
    "rollup": "^2.26.3"
  },
  "keywords": [
    "querySelector",
    "querySelectorAll",
    "addEventListener",
    "event",
    "events",
