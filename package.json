{
  "name": "quick-selector",
  "version": "1.0.8",
  "description": "Syntactic sugar for querySelector, querySelectorAll, & addEventListener allowing you to select, modify, and add event listeners for multiple elements in one line of code. Tiny package (<1KB GZIP).",
  "homepage": "https://github.com/CTNicholas/quick-selector#readme",
  "bugs": {
    "url": "https://github.com/CTNicholas/quick-selector/issues"
  },
  "source": "src/index.js",
  "main": "dist/qs.js",
  "module": "dist/qs.module.js",
  "unpkg": "dist/qs.umd.js",
  "scripts": {
    "build": "microbundle --name qs",
    "dev": "microbundle watch --name qs",
    "test": "jest --watch"
  },
  "author": "CTNicholas",
  "license": "ISC",
  "devDependencies": {
    "jest": "^26.1.0",
    "microbundle": "^0.12.3"
  },
  "dependencies": {},
  "keywords": [
    "querySelector",
    "querySelectorAll",
    "addEventListener",
    "event",
    "events",
    "elements",
    "quick",
    "query",
    "selector",
    "select",
    "dom",
    "iterate",
    "browser"
  ]
}
